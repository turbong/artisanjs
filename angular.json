{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@craftsjs/ngrx-action": {
      "projectType": "library",
      "root": "projects/craftsjs/ngrx-action",
      "sourceRoot": "projects/craftsjs/ngrx-action/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/craftsjs/ngrx-action/tsconfig.lib.json",
            "project": "projects/craftsjs/ngrx-action/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/craftsjs/ngrx-action/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/craftsjs/ngrx-action/src/test.ts",
            "tsConfig": "projects/craftsjs/ngrx-action/tsconfig.spec.json",
            "karmaConfig": "projects/craftsjs/ngrx-action/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/craftsjs/ngrx-action/tsconfig.lib.json",
              "projects/craftsjs/ngrx-action/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
