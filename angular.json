    "@artisanjs/ngrx-action": {
      "projectType": "library",
      "root": "projects/artisanjs/ngrx-action",
      "sourceRoot": "projects/artisanjs/ngrx-action/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/artisanjs/ngrx-action/tsconfig.lib.json",
            "project": "projects/artisanjs/ngrx-action/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/artisanjs/ngrx-action/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/artisanjs/ngrx-action/src/test.ts",
            "tsConfig": "projects/artisanjs/ngrx-action/tsconfig.spec.json",
            "karmaConfig": "projects/artisanjs/ngrx-action/karma.conf.js"
          }
